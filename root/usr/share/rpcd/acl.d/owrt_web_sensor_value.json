{
  "owrt_web_sensor_value": {
    "description": "UCI access to LuCI app",
    "read": {
      "ubus": {
        "uci": [
          "get", "access"
        ],
        "owrt_sensor_value": [ "get_value" ]
      },
      "uci": [ "owrt_sensor_value" ],
      "cgi-io": [ "exec" ],
      "file": {
        "ubus call uci get {'config':'owrt_sensor_value'}": [ "exec" ],
      }
    },
    "write": {
      "ubus": {
        "uci": [ "set", "commit" ]
      },
      "uci": [ "owrt_sensor_value" ]
    }
  }
}
